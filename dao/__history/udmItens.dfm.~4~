object DataModule2: TDataModule2
  Height = 480
  Width = 640
  object fdqItens: TFDQuery
    Connection = dmConexao.fdConexao
    SQL.Strings = (
      'select *'
      'from itens'
      'where codpedido = :pdocpedido;')
    Left = 56
    Top = 40
    ParamData = <
      item
        Name = 'PDOCPEDIDO'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
end
